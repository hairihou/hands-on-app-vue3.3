{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "js": {
      "transformGroup": "js",
      "buildPath": "build/js/",
      "files": [
        {
          "destination": "variables.js",
          "format": "javascript/esm",
          "options": {
            "minify": true
          }
        }
      ]
    }
  }
}
